    {
      "provider": "cdnjs",
      "library": "twitter-bootstrap@4.1.3",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "js/bootstrap.min.js.map",
        "js/bootstrap.min.js",
        "css/bootstrap.css",
